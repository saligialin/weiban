<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbteam.weiban.mapper.CityMapper">

    <select id="getAdCodeByCityName" resultType="java.lang.String">
        SELECT ad_code FROM weiban_city WHERE city_name = #{cityName}
    </select>

</mapper>