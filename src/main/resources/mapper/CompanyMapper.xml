<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbteam.weiban.mapper.CompanyMapper">

    <select id="getList" resultType="com.wbteam.weiban.entity.Company">
        SELECT * FROM weiban_company
    </select>
    <select id="getCompanyById" resultType="com.wbteam.weiban.entity.Company">
        SELECT * FROM weiabn_company WHERE id = #{id}
    </select>
</mapper>