<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbteam.weiban.mapper.MemoKindMapper">

    <select id="getList" resultType="com.wbteam.weiban.entity.MemoKind">
        SELECT * FROM weiban_memo_kind
    </select>
    <select id="getMemoKindById" resultType="com.wbteam.weiban.entity.MemoKind">
        SELECT * FROM weiban_memo_kind WHERE id = #{id}
    </select>
</mapper>